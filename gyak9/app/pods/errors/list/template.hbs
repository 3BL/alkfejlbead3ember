{{#each messages}}
<div class="alert alert-dismissible alert-success">
    <button type="button" class="close" data-dismiss="alert">×</button>
    {{this}}
</div>
{{/each}}

<div class="page-header">
    <h1>Bejelentett chipek</h1>
</div>

{{log model}}
{{error-list errors=model}}

<button {{action 'showModal' 'new-error-modal' "errors.list"}} class="btn btn-default">Új chip felvétele</button>
